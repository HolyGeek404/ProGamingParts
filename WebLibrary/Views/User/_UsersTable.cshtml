@model List<Model.DataTransfer.UserDto>

<h2 class="font-weight-light ml-1 ">Użytkownicy i Zamówienia</h2>

<table id="usersTable" class="table table-striped">
    <thead>
    <tr>
        <th>ID</th>
        <th>Imie</th>
        <th>Email</th>
        <th>Actions</th>
    </tr>
    </thead>
    <tbody></tbody>
</table>
<script type="text/javascript">
    var users = @Json.Serialize(Model);

    $(document).ready(function() {
        CreateUsersTable(users);
        $('#usersTable tbody').on('click', '#loadUserOrders', LoadUserOrders);
        $('#usersTable tbody').on('click', '#deleteUser', DeleteUser);
        $('#usersTable tbody').on('click', '#editUser', LoadUser);
    });

</script>